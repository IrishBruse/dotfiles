  "editor.semanticTokenColorCustomizations": {
    "[Empty Dark Theme]": {
      "enabled": true,
      "rules": {
        "punctuation": "#ABB2BF",
        "enum": "#E5C07B",
        "boolean": "#56B6C2",
        "number": "#D19A66",
        "operatorOverloaded": {
          "bold": true,
          "foreground": "#C678DD",
        },
        "variable.defaultLibrary": "#E5C07B",
        "parameter": "#E06C75",
        "parameter.declaration": "#ABB2BF",
      },
    },
  },
